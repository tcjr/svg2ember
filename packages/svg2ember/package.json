{
  "name": "svg2ember",
  "version": "0.1.1",
  "description": "Transform SVGs into Ember components.",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.js"
    },
    "./vite": {
      "types": "./dist/vite/index.d.ts",
      "import": "./dist/vite/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "svg-parser": "^2.0.4",
    "svgo": "^3.2.0",
    "commander": "^12.0.0",
    "glob": "^11.0.0"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0",
    "rollup": "^4.0.0",
    "vite": "^5.0.0"
  },
  "keywords": [
    "svg",
    "ember",
    "component",
    "transform",
    "ast",
    "cli",
    "vite",
    "plugin"
  ],
  "author": "Tom Carter (https:/tcjr.org/)",
  "repository": "https://github.com/tcjr/svg2ember.git",
  "license": "MIT"
}
