{
  "name": "@svg2ember/vite",
  "version": "0.1.0",
  "description": "Vite plugin for converting SVG imports to Ember components",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@svg2ember/core": "workspace:*"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "rollup": "^4.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.2.0"
  },
  "keywords": [
    "vite",
    "plugin",
    "svg",
    "ember",
    "component",
    "transform"
  ],
  "author": "",
  "license": "MIT"
}
