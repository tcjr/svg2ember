{
  "name": "@svg2ember/core",
  "version": "0.1.0",
  "description": "Core transformation logic for converting SVG files to Ember components",
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "svg-parser": "^2.0.4",
    "svgo": "^3.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "keywords": [
    "svg",
    "ember",
    "component",
    "transform",
    "ast"
  ],
  "author": "",
  "license": "MIT"
}
